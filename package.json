  "bugs": {
    "url": "https://github.com/JosunLP/ZDF-enable-pip/issues"
  },
  "homepage": "https://github.com/JosunLP/ZDF-enable-pip#readme",
  "devDependencies": {
    "@types/node": "^20.14.0",
    "typescript": "^4.7.4",
    "vite": "^5.2.12",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "volta": {
    "node": "20.14.0"
  }
